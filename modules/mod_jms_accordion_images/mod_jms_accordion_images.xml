<?xml version="1.0" encoding="utf-8"?>
<install type="module" version="1.5.0">
	<name>JMS Accordion Images</name>
	<author>Joomla! Project</author>
	<creationDate>Jan 2010</creationDate>
	<copyright>ww.joommasters.com</copyright>
	<license>ww.joommasters.com</license>
	<authorEmail>joommasters@gmail.com</authorEmail>
	<authorUrl>www.joommasters.com</authorUrl>
	<version>1.0</version>
	<description>JMS Accordion Images</description>
	<files>
		<filename module="mod_jms_accordion_images">mod_jms_accordion_images.php</filename>
		<filename module="mod_jms_accordion_images">index.html</filename>
		<filename module="mod_jms_accordion_images">helper.php</filename>
		<filename module="mod_jms_accordion_images">tmpl/default.php</filename>
		<filename module="mod_jms_accordion_images">tmpl/index.html</filename>
		<filename module="mod_jms_accordion_images">assets/common.css</filename>
		<filename module="mod_jms_accordion_images">assets/jquery-1.2.3.js</filename>
	</files>
	<params>
		<param name="height" type="text" default="260" label="Height" description="Height" />
		<param name="width" type="text" default="923" label="Width" description="Width" />
		<param name="desc_height" type="text" default="30" label="Desc Height" description="Desc Height" />
		<param name="maxwidth" type="text" default="260" label="Max width" description="Max width" />
		<param name="description" type="textarea" cols="50" rows="20" default="" label="DESCRIPTION FOR EACH ITEM" description="DESCRIPTION FOR EACH ITEM DESC" />
		<param name="@spacer" type="spacer" default="" label="" description="" />
		<param name="moduleclass_sfx" type="text" default="" label="Module Class Suffix" description="PARAMMODULECLASSSUFFIX" />
	</params>
	<params group="advanced">
		<param name="cache" type="list" default="1" label="Caching" description="Select whether to cache the content of this module">
			<option value="1">Use global</option>
			<option value="0">No caching</option>
		</param>
		<param name="cache_time" type="text" default="900" label="Cache Time" description="The time before the module is recached" />
	</params>
</install>
